{
  "extends": [
    "prettier-standard/prettier-file",
    "plugin:import/errors",
    "plugin:import/warnings",
    "eslint:recommended"
  ],
  "plugins": ["import"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {}
}
